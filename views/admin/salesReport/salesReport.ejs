<script>
  $(document).ready(function() {
    // Function to update the table with the received data
    function updateTable(data) {
      var tbody = $('.table.table-dark tbody'); // Select the tbody element

      // Clear the previous data in the tbody
      tbody.empty();

      // Check if data is empty
      if (data.orderDetails.length === 0) {
        // If there are no sales data, display "Sales Not Found" message
        tbody.append('<tr><td colspan="7" style="font-family: \'Lucida Sans\', \'Lucida Sans Regular\', \'Lucida Grande\', \'Lucida Sans Unicode\', Geneva, Verdana, sans-serif;color: rgba(225, 57, 57, 0.824);">Sales Not found</td></tr>');
      } else {
        // Loop through the received data and add new rows with the sales report data
        for (let i = 0; i < data.orderDetails.length; i++) {
          const order = data.orderDetails[i];
          const statusColor = order.status === 'Pending' ? '#cf7c558a' : '#55cf768a';
          const formattedDate = formatDate(order.orderdate);

          // Create a string that contains all the product names separated by line breaks
          const productNames = order.product.map(prod => prod.product_name).join('<br>');

          const row = '<tr>' +
            '<td>' + (i + 1) + '</td>' +
            '<td>' + formattedDate + '</td>' +
            '<td>' + order.name + '</td>' +
            '<td>' + order.payment_method + '</td>' +
            '<td>' + productNames + '</td>' + // Add the combined product names to the row
            '<td style="color: ' + statusColor + ';">' + order.status + '</td>' +
            '<td style="color: rgba(175, 197, 216, 0.758);font-family: \'Segoe UI\', Tahoma, Geneva, Verdana, sans-serif;">₹ ' + order.grandTotal + '.00</td>' +
            '</tr>';
          tbody.append(row);
        }
      }
    }

    // Function to fetch sales report data based on the selected date range
    function fetchSalesReportData(fromDate, toDate) {
      $.ajax({
        url: '/admin/getSalesReportData',
        type: 'GET',
        data: { fromDate: fromDate, toDate: toDate },
        dataType: 'json',
        success: function(data) {
          updateTable(data); // Update the table with the received data
        },
        error: function(xhr, status, error) {
          console.error(error);
        }
      });
    }

    // Add an event listener to the form submission
    $('#contactForm').on('submit', function(event) {
      event.preventDefault(); // Prevent the default form submission behavior

      var fromDate = $('#fromDate').val(); // Get the selected "From Date" value
      var toDate = $('#toDate').val(); // Get the selected "To Date" value

      // Fetch sales report data for the selected date range
      fetchSalesReportData(fromDate, toDate);
    });

    // Click event handler for "Today's Sales"
    $('#todaysSalesBtn').on('click', function() {
      var currentDate = new Date();
      var fromDate = currentDate.toISOString().split('T')[0];
      var toDate = currentDate.toISOString().split('T')[0];

      // Fetch sales report data for today's sales
      fetchSalesReportData(fromDate, toDate);
    });

    $('#totalSalesBtn').on('click', function() {
      // Refresh the page
      window.location.reload();
    });

    // Click event handler for "Monthly Sales"
    $('#monthlySalesBtn').on('click', function() {
      var currentDate = new Date();
      var firstDayOfMonth = new Date(currentDate.getFullYear(), currentDate.getMonth(), 1);
      var lastDayOfMonth = new Date(currentDate.getFullYear(), currentDate.getMonth() + 1, 0);
      var fromDate = firstDayOfMonth.toISOString().split('T')[0];
      var toDate = lastDayOfMonth.toISOString().split('T')[0];

      // Fetch sales report data for the current month's sales
      fetchSalesReportData(fromDate, toDate);
    });

    // Click event handler for "Yearly Sales"
    $('#yearlySalesBtn').on('click', function() {
      var currentDate = new Date();
      var firstDayOfYear = new Date(currentDate.getFullYear(), 0, 1);
      var lastDayOfYear = new Date(currentDate.getFullYear(), 11, 31);
      var fromDate = firstDayOfYear.toISOString().split('T')[0];
      var toDate = lastDayOfYear.toISOString().split('T')[0];

      // Fetch sales report data for the current year's sales
      fetchSalesReportData(fromDate, toDate);
    });

    function getMonthName(monthNumber) {
      const monthNames = [
        'January', 'February', 'March', 'April', 'May', 'June', 'July',
        'August', 'September', 'October', 'November', 'December'
      ];

      return monthNames[monthNumber - 1];
    }

    function formatDate(dateString) {
      const date = new Date(dateString);
      const day = String(date.getDate()).padStart(2, '0');
      const month = getMonthName(date.getMonth() + 1); // Get the month in words
      const year = date.getFullYear();
      return `${day} ${month} ${year}`;
    }
  });
</script>



