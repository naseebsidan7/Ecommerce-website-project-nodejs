    <script>
      $(document).ready(function() {
        var canvas = document.getElementById("transaction-history");
        var chartData = JSON.parse(canvas.getAttribute("data-chartdata"));
    
        var transactionhistoryChartCanvas = canvas.getContext("2d");
        var transactionhistoryChart = new Chart(transactionhistoryChartCanvas, {
          type: 'doughnut',
          data: {
            labels: chartData.labels,
            datasets: [{
              data: chartData.data,
              backgroundColor: ['#111111', '#00d25b', '#ffab00'], // Customize as needed
            }],
          },
          // Other chart options...
        });
      });
    </script>

