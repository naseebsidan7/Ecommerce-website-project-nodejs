<script>
  function showOrderStatusBox() {
    var productId = $('#idForServer').val();
    // Use the productId as needed
    console.log(productId);
  
    // Show the order status box/modal
    $('#orderStatusModal').modal('show');
  }
  function updateOrderStatus() {
  var selectedStatus = $('input[name="statusOption"]:checked').val();
  var productId = $('#idForServer').val();

  $.ajax({
    url: '/admin/updateOrderStatus',
    method: 'POST',
    data: { status: selectedStatus, id: productId },
    success: function(response) {
      console.log('Order status updated successfully.');

      // Update the page content dynamically
      $('#statusText').val(selectedStatus);

      // Perform any further actions upon successful update
      swal('Product status is changed', 'success');
      console.log('Data sent successfully!');
    },
    error: function(error) {
      // Handle the error response
      console.log('Error:', error);
    }
  });

  $('#orderStatusModal').modal('hide');
}

</script>


  
 
  

