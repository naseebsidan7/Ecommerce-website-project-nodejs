        <div class="form h-100">
            <h2 style="margin-bottom: 5%; color: rgba(240, 248, 255, 0.811);">Product Details</h2>

            
            <div class="confirmBox" id="confirmBox">
              <div class="confirmContent">
                <h3>Confirmation</h3>
                <p>Are you sure you want to cancel this Product</p>
                <div class="confirmActions">
                  <button onclick="confirmAction(true)">Yes</button>
                  <button onclick="confirmAction(false)">No</button>

                </div>
              </div>
            </div>
            
       <% Product.forEach(product => { %>
            <hr style="border: none; height: 1px; background-color: rgba(255, 255, 255, 0.208); margin: 50px 0;">
          <form class="mb-5 " id="contactForm" name="contactForm" >

            <h3 style="color: aquamarine; font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif; margin-bottom: 4%; "><%= product.product_name %></h3>
            <div class="row">
            
              <div class="col-md-6 form-group mb-3" >
                <label for="" class="col-form-label">Brand Name </label>
                <input type="text" class="form-control" name="brand" id="name" value="<%= product.brand %>" readonly style="color: rgba(255, 255, 255, 0.644); background-color: #1b1e20af;font-family: 'Courier New', Courier, monospace; font-size: larger;">
              </div>
              <div class="col-md-6 form-group mb-3">
                <label for="" class="col-form-label"> Product Total Price </label>
                <input type="number" class="form-control" name="price"  value="<%= product.total %>" style="color: rgba(255, 255, 255, 0.644); background-color: #1b1e20af;font-family: 'Courier New', Courier, monospace;  font-size: larger;" readonly >
              </div>
            </div>



            <!-- productDetails.ejs -->

  
         
         
           
            

       <div class="row" style="padding: 20px;margin-top: 30px;margin-bottom: 30px">
                <!-- single image start-->
                
                <% for (let i = 0; i < product.image.length; i++) { %>
                    <div class="col-sm-6 col-md-4 col-lg-3 p-b-35 isotope-item">
                      <!-- Block2 -->
                      <div class="block2">
                        <div class="block2-pic hov-img0" style="margin-left: 0;margin-top: 20px;">
                        
                          <img src="/public/product-images/<%= product.image[i] %>" alt="IMG-PRODUCT" height="200px" width="160px">
                        </div>
                      </div>
                    </div>
                  <% } %>
                
              </div>

              <input type="text" value="<%= product.productid %>" name="id" hidden id="idForServer">

             
                <!-- single image end -->
       
         
            <div class="row">
            <div class="col-md-6 form-group mb-3">
              <label for="" class="col-form-label">Size </label>
              <input type="text" class="form-control" name="size" id="name" value=" <%= product.size %>" readonly style="color: rgba(255, 255, 255, 0.644); background-color: #1b1e20af;font-family: 'Courier New', Courier, monospace;  font-size: larger;">
            </div>
          </div>


            <div class="row">
              <div class="col-md-6 form-group mb-3">
                <div class="input-group">
                  <label for="" class="col-form-label"  style="margin-bottom: -10px;
                  margin-top: 40px;  color: rgba(240, 124, 124, 0.717); font-size: 16px;">Product Status</label>
              
              </div>
              </div>
            </div>

            <div class="row">
              <div class="col-md-6 form-group mb-5">
                <div class="input-group">
                  
                  <input type="text" class="form-control" name="Category" id="statusText" value="<%= product.status %>" style="color: rgba(255, 255, 255, 0.644); background-color: #1b1e20af; font-family: 'Courier New', Courier, monospace; font-size: larger;" readonly>
                  
                   

                </div>
              </div>
            </div>


            
          
        
            <div style="display: flex;">
              <div style="flex: 1;">
                <h2 style="margin-bottom: 5%; color: rgba(240, 248, 255, 0.811); margin-top: 40px;">Order Address</h2>
                <ul style="color: rgba(142, 172, 255, 0.815);">
                  <% AddressDetails.forEach(addr => { %>
                  <li style="margin-bottom: 4px;">State: <%= addr.state %></li>
                  <li style="margin-bottom: 4px;">Location: <%= addr.location %></li>
                  <li style="margin-bottom: 4px;">Landmark: <%= addr.landmark %></li>
                  <li style="margin-bottom: 4px;">City: <%= addr.city %></li>
                  <li style="margin-bottom: 4px;">Pincode: <%= addr.pincode %></li>
                  <li style="margin-bottom: 4px;">Country: <%= addr.country %></li>
                  <li style="margin-bottom: 4px;">Mobile: <%= addr.mobile %></li>
                  <% }); %>
                </ul>
              </div>
            
              <div style="flex: 1;">
