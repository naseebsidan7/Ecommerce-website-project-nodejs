	function handleCouponValidation() {
		var couponCode = $('#couponInput').val();
		var subtotal = parseFloat($('#subTotalAmount').text().replace('₹', '').trim());

	 
	  // Make an AJAX call to the coupon validation route
	  $.ajax({
		url: "/validateCoupon",
		type: "post",
		data: { coupon: couponCode, subtotal: subtotal }, // Pass the subtotal to the server
		success: function (res) {
		  if (res.msg==="1" ) {
			 
			var couponAmount = res.couponAmount;
			console.log(couponAmount+"couponAmount");	
			var discountedAmount = subtotal - couponAmount;
  
			// Update the displayed values
			$("#finalTotalAmount").text("₹" + discountedAmount.toFixed(2));
			$("#couponAmount").text("-₹" + couponAmount.toFixed(2));
			$("#discountedAmountInput").val(discountedAmount.toFixed(2));
  
			Swal.fire({
			  icon: 'success',
			  title: 'Coupon applied successfully',
			  text: 'Discounted amount: ₹' + discountedAmount.toFixed(2)
			});
		  } else {
			// Reset the displayed values if the coupon is invalid
			$("#finalTotalAmount").text("₹ " + subtotal.toFixed(2)); // Use the original subtotal
			$("#couponAmount").text("₹ 0");
  
			Swal.fire({
			icon: 'error',
			title: 'Coupon validation error',
			 html: '<span style="color: #FF0000;">' + res.message + '</span>'
			});

		  }
		},
		error: function (error) {
		  console.log(error.responseText);
		  // Reset the displayed values in case of an error
		  $("#finalTotalAmount").text("₹" + subtotal.toFixed(2)); // Use the original subtotal
		  $("#discountAmount").text("₹ 0");
  
		  Swal.fire({
			icon: 'error',
