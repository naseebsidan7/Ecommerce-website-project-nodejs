	<section class="checkout spad">
		<div class="container">
			<div class="checkout__form">
				<form action="checkoutOrder" method="post" class="Order_form" id="orderForm" onsubmit="return handleFormSubmit()">

					<div class="row">
						<div class="col-lg-8 col-md-6" style="margin-left: -50px;">
							<h6 class="coupon__code"><span class="icon_tag_alt"></span> Have a coupon? <a href="#">Click
									here</a> to enter your code</h6>
							<h6 class="checkout__title" style="font-family: Nunito Sans, sans-serif">Billing Details</h6>
							<div class="row">
								<div class="col-lg-6">
									<div class="checkout__input">
										<p>Fist Name<span>*</span></p>
										<input type="text" name="name" value="<%= userData.name %>"
											style="font-size: 18px; color: rgba(55, 84, 153, 0.664);font-family:  Verdana, Geneva, Tahoma, sans-serif;  border: 1px solid rgba(0, 0, 0, 0.288);"
											required>
									</div>
								</div>
								<div class="col-lg-6">
									<div class="checkout__input">
										<p>State<span>*</span></p>
										<input type="text" name="state"
											style="font-size: 18px; color: rgba(55, 84, 153, 0.664);font-family: Verdana, Geneva, Tahoma, sans-serif; border: 1px solid rgba(0, 0, 0, 0.288);"
											value="<%= address.state %>" required>
									</div>
								</div>
							</div>
							<div class="checkout__input">
								<p>Country<span>*</span></p>
								<input type="text" name="country"
									style="font-size: 18px; color: rgba(55, 84, 153, 0.664);font-family: Verdana, Geneva, Tahoma, sans-serif; border: 1px solid rgba(0, 0, 0, 0.288);"
									value="<%= address.country %>" required>
							</div>
							<div class="checkout__input">
								<p>Address<span>*</span></p>
								<input type="text" name="location" placeholder="Street Address"
									class="checkout__input__add"
									style="font-size: 18px; color: rgba(55, 84, 153, 0.664);font-family: Verdana, Geneva, Tahoma, sans-serif;; border: 1px solid rgba(0, 0, 0, 0.288);"
									value="<%= address.location %>" required>
								<p>LandMark<span></span></p>
								<input type="text" name="landmark" placeholder="Apartment, suite, unite ect (optinal)"
									style="font-size: 18px; color: rgba(55, 84, 153, 0.664);font-family:  Verdana, Geneva, Tahoma, sans-serif;  border: 1px solid rgba(0, 0, 0, 0.288);"
									value="<%= address.landmark %>" required>
							</div>
							<div class="checkout__input">
								<p>Town/City<span>*</span></p>
								<input type="text" name="city"
									style="font-size: 18px; color: rgba(55, 84, 153, 0.664);font-family:  Verdana, Geneva, Tahoma, sans-serif;  border: 1px solid rgba(0, 0, 0, 0.288);"
									value="<%= address.city %>" required>
							</div>

							<div class="checkout__input">
								<p>Pincode / ZIP<span>*</span></p>
								<input type="text" pattern="[0-9]{6}" name="pincode"
									style="font-size: 18px; color: rgba(55, 84, 153, 0.664);font-family:  Verdana, Geneva, Tahoma, sans-serif;  border: 1px solid rgba(0, 0, 0, 0.288);"
									value="<%= address.pincode %>" required>
							</div>
							<div class="row">
								<div class="col-lg-6">
									<div class="checkout__input">
										<p>Phone<span>*</span></p>
										<input type="tel" name="mobile"
											style="font-size: 18px; color: rgba(55, 84, 153, 0.664);font-family:  Verdana, Geneva, Tahoma, sans-serif;  border: 1px solid rgba(0, 0, 0, 0.288);"
											value="<%= address.mobile %>" required>
									</div>
								</div>
								<div class="col-lg-6">
									<div class="checkout__input">
										<p>Email<span>*</span></p>
										<input type="text" name="email"
											style="font-size: 18px; color: rgba(55, 84, 153, 0.664);font-family:  Verdana, Geneva, Tahoma, sans-serif;  border: 1px solid rgba(0, 0, 0, 0.288);"
											value="<%= userData.email %>" required>
									</div>
								</div>
							</div>
							<!-- <div class="checkout__input__checkbox">
                                <label for="acc">
                                    Create an account?
                                    <input type="checkbox" id="acc">
                                    <span class="checkmark"></span>
                                </label>
                                <p>Create an account by entering the information below. If you are a returning customer
                                please login at the top of the page</p>
                            </div> -->
							<!-- <div class="checkout__input">
                                <p>Account Password<span>*</span></p>
                                <input type="password"   name="password" style="font-size: 18px; color: rgba(55, 84, 153, 0.664);font-family: monospace; border: 1px solid rgba(0, 0, 0, 0.288);"  >
                            </div> -->


							<!-- <div class="checkout__input__checkbox">
                                <label for="diff-acc">
                                    Note about your order, e.g, special noe for delivery
                                    <input type="checkbox" id="diff-acc">
                                    <span class="checkmark"></span>
                                </label>
                            </div> -->
							<!-- <div class="checkout__input">
                                <p>Order notes<span>*</span></p>
                                <input type="text"
                                placeholder="Notes about your order, e.g. special notes for delivery.">
                            </div> -->
						</div>

						<div>

						</div>
						<div class="col-lg-4 col-md-6">
							<div class="checkout__order"
								style="background-color: rgba(239, 243, 250, 0.968);border: 1px solid rgba(0, 0, 0, 0.137); margin-right: -105px; ">
								<h4 class="order__title">Your order</h4>
								<div class="checkout__order__products" style="font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif;">Product <span>Total</span></div>
								<ul class="checkout__total__products">
									<% for (let i=0; i < cartProducts.length; i++) { %>

										<input type="text" hidden name="ProductName"
											style="font-size: 18px; color: rgba(55, 84, 153, 0.664);font-family:  Verdana, Geneva, Tahoma, sans-serif;  border: 1px solid rgba(0, 0, 0, 0.288);"
											value="<%= cartProducts[i].name %> ">
										<input type="text" hidden name="Productprice"
											style="font-size: 18px; color: rgba(55, 84, 153, 0.664);font-family:  Verdana, Geneva, Tahoma, sans-serif;  border: 1px solid rgba(0, 0, 0, 0.288);"
											value="<%= cartProducts[i].price %> ">
										<input type="text" hidden name="ProductQnty"
											style="font-size: 18px; color: rgba(55, 84, 153, 0.664);font-family:  Verdana, Geneva, Tahoma, sans-serif;  border: 1px solid rgba(0, 0, 0, 0.288);"
											value="<%= cartProducts[i].quantity %>">
										<input type="text" hidden name="finalAmount"  
											style="font-size: 18px; color: rgba(55, 84, 153, 0.664);font-family:  Verdana, Geneva, Tahoma, sans-serif;  border: 1px solid rgba(0, 0, 0, 0.288);"
											value="<%= finalAmount %>">



										<li style="color: rgba(255, 0, 0, 0.772);"> x <%= cartProducts[i].quantity %> 
											<span style="margin-left: 25px">₹ <%= cartProducts[i].price %></span>	<span style="color: rgba(71, 70, 70, 0.874);">	<%= cartProducts[i].name %> </span>
										</li>

										<% } %>
								</ul>
								<ul class="checkout__total__all">

									<li style="margin-top: 30px; font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif;">Subtotal <span id="subTotalAmount" style="color: rgba(67, 72, 147, 0.817); font-family: monospace;">₹ <%= finalAmount %>.00</span></li>
									<p class="stext-111 cl6 p-t-2" style="font-size: 12px; color: rgba(106, 113, 113, 0.769);">Includes GST and shipping charge</p>

									<li style="margin-top: 30px; font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif;">Discount <span id="couponAmount" style="color: rgba(67, 72, 147, 0.817); font-family: monospace;">₹ 0</span></li>
									
									<li style="font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif;">Total <span id="finalTotalAmount">₹ <%= finalAmount %>.00</span></li>
									<!-- Add this hidden input field after the "Total" line -->
                                    <li style="display: none;"><input type="hidden" id="discountedAmountInput" name="discountedAmount" value=""></li>
									<input type="text" hidden id="couponAppliedamount" name="couponAppliedamount" value="">

								  </ul>
								  
								  
																<!-- Add an ID to the button for easier selection -->
								<div class="flex-w flex-sb-m bor15 p-t-18 p-b-15 p-lr-40 p-lr-15-sm">
									<div class="flex-w flex-m m-r-20 m-tb-5">
									<input class="stext-104 cl2 plh4 size-117 bor13 p-lr-20 m-r-10 m-tb-5" type="text" name="coupon" id="couponInput" placeholder="Coupon Code" style="border: rgba(0, 0, 0, 0.514) 1px solid; color: rgba(0, 0, 0, 0.792);margin-left: -21px;width: 179px; font-size: 18px; font-family:'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;">
									<div class="flex-c-m stext-101 cl2 size-118 bg8 bor13 hov-btn3 p-lr-15 trans-04 pointer m-tb-5" style="min-width: 143px; border: rgba(0, 0, 0, 0.514) 1px solid;" id="applyCouponBtn" onclick="handleCouponValidation()">
										Apply coupon
									</div>
									</div>
								</div>
  
 
  
								  
								<!-- <div class="checkout__input__checkbox">
                                    <label for="acc-or">
                                        Create an account?
                                        <input type="checkbox" id="acc-or">
                                        <span class="checkmark"></span>
                                    </label>
                                </div> -->

								<h4
									style="color: rgba(0, 0, 0, 0.863); font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif; margin-top: 55px; margin-bottom: 30px; margin-left: 20px;">
									Payment Method</h4>
								<hr style="height: 0.1px; background-color: rgba(0, 0, 0, 0.107);">

								<div class="checkout__input__checkbox" style="margin-bottom: 20px;margin-top: 40px;">
									<label for="payment" >
										Check Payment
										<input type="radio" id="payment" name="payment" value="check"
											onchange="handleCheckboxChange('payment')">
										<span class="checkmark"></span>
									</label>
								</div>

								<div class="checkout__input__checkbox" style="margin-bottom: 20px; margin-top: 30px;">
									<label for="paypal">
										Razorpay
										<input type="radio" id="paypal" name="payment" value="Razorpay"
											onchange="handleCheckboxChange('paypal')">
										<span class="checkmark"></span>
									</label>
								</div>

								<div class="checkout__input__checkbox" style="margin-bottom: 50px; margin-top: 30px;">
									<label for="cod">
										Cash on Delivery
										<input type="radio" id="cod" name="payment" value="cod"
											onchange="handleCheckboxChange('cod')">
										<span class="checkmark"></span>
									</label>
								</div>

								<button type="submit" class="site-btn">PLACE ORDER</button>
							</div>
						</div>
					</div>
				</form>
			</div>
		</div>
	</section>
