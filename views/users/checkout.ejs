    <script>
	function handleFormSubmit() {
	  var paymentMethod = $('input[name="payment"]:checked').val();
		if (paymentMethod === 'cod') {
                 
			 // For Cash on Delivery (COD) payment method, we need to update the form with the discounted amount
			  var discountedAmount = $("#discountedAmountInput").val();
			  var couponAmountElement = document.getElementById("couponAmount");
			  
			  var couponAmountValue = couponAmountElement.textContent;
			// For Cash on Delivery (COD) payment method, we need to update the displayed final amount with the discounted amount
			$("#finalTotalAmount").text("₹ " + discountedAmount); // Update the displayed final amount in the <span> element
			// Update the finalAmount input field in the form with the discounted amount
			$("input[name='finalAmount']").val(discountedAmount);

			$("input[name='couponAppliedamount']").val(couponAmountValue);

			return true; // Allow form submission for Cash on Delivery (cod) payment method
			

	} else if (paymentMethod === 'Razorpay') {
		// Handle Razorpay payment method
		var formData = $('#orderForm').serialize();
		var discountedAmount = $("#discountedAmountInput").val();
		var couponAmountElement = document.getElementById("couponAmount");
			  var couponAmountValue = couponAmountElement.textContent;
			// For Cash on Delivery (COD) payment method, we need to update the displayed final amount with the discounted amount
			$("#finalTotalAmount").text("₹ " + discountedAmount); // Update the displayed final amount in the <span> element
			// Update the finalAmount input field in the form with the discounted amount
			$("input[name='finalAmount']").val(discountedAmount);

			$("input[name='couponAppliedamount']").val(couponAmountValue);
			
        formData += "&amount=" + discountedAmount;
		$.ajax({
		  url: "/checkoutOnline", 
		  type: "post",
		  data: formData,
		  success: function (res) {
			if (res.success) {
			  var options = {
				"key": "" + res.key_id + "",
				"amount": "" + res.amount + "",
				"currency": "INR",
				"name": "My Store",
				"description": "Test Transaction",
				"image": "/public/icons/My project.png",
				"order_id": "" + res.order_id + "",
				"handler": function (response) {
				  Swal.fire({
					icon: 'success',
					title: 'Payment success',
					showConfirmButton: false,
					timer: 1500
				  }).then(function() {
					// Submit the form after successful payment
					document.getElementById('orderForm').submit();
				  });
				},
				"prefill": {
				  "contact": "" + res.contact + "",
				  "name": "" + res.name + "",
				  "email": "" + res.email + ""
				},
				"theme": {
				  "color": "#404fc2"
				}
			  };
			  var razorpayObject = new Razorpay(options);
			  razorpayObject.on('payment.failed', function (response) {
				Swal.fire({
				  icon: 'error',
				  title: 'Payment failed',
				  showConfirmButton: false,
				  timer: 1500
				});
			  });
			  razorpayObject.open();
			} else {
			  Swal.fire({
				icon: 'error',
				title: 'Error',
				text: res.message
			  });
			}
		  }
		});
		return false; // Prevent form submission
	  } else {
		return false; // Prevent form submission for other payment methods
	  }
	}
    </script>
  
	
	  
  
	  
