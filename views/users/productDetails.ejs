	<script>
		$(document).ready(function () {
		  $('#addcart').on('click', function (e) {
			console.log("inside");
			e.preventDefault();
	  
			var action = $(this).data('action');
			var selectedSize = $('#sizeDropdown').val();
			var selectedQnty = $('#qnty-number').val();
	  
			console.log("qnty", selectedQnty);
			console.log("size", selectedSize);
	  
			if (selectedSize === 'Choose an option') {
			  $('#size-error-msg').show();
			  return;
			} else {
			  $('#size-error-msg').hide();
			}
	  
			var clickedElement = $(this); // Store the reference to the clicked element
	  
			$.ajax({
			  url: '/addtocart',
			  method: 'POST',
			  data: {
				id: '<%= Product._id %>',
				type: 'addtocart',
				size: selectedSize,
				qnty: selectedQnty,
				price: '<%= Product.price %>'
			  },
			  success: function (response) {

				if (response.ok) {

				var nameProduct = clickedElement.parent().parent().parent().parent().find('.js-name-detail').html();
				swal(nameProduct, "is added to cart!", "success");
	  
				console.log('Data sent successfully!');
				// Refresh the page after successful addition to the cart
				setTimeout(function () {
					window.location.reload();
				}, 1000);
               
			} else {

				swal({
				title: "Error",
				text: "Please login / Signup",
				icon: "error"
		     	});
				
			  }

			  },
			  error: function (error) {
				// Handle the error response
				console.log('Error:', error);
			  }
			});
		  });
		});
	  
	 
	  </script>
	  
